<h1>Lista de Cadastros</h1>

<div class="container">
	<div class="valores">
		{{#each cadastros}}
		 <div class="cadastro_individual">
			ID:  {{dataValues.userId}}<br>
			Nome: {{dataValues.nome}}<br>
			E-mail: {{dataValues.email}}<br>
			Telefone: {{dataValues.fone}}<br>
			Senha: {{dataValues.senha}}<br>
			Cadastrado: {{#formatDate dataValues.createdAt}}{{/formatDate}}
			<a href="/edit-cadastro/{{dataValues.userId}}"><button class="btn btn-outline-primary" id="edit-button">Editar</button></a>
			<a href="/del-cadastro/{{dataValues.userId}}"><button class="btn btn-outline-danger" id="delete-button">Apagar</button></a>
		 </div>
		{{/each}}
	</div>
</div>